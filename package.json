{
  "name": "personnummer",
  "description": "Validate Swedish personal identity numbers",
  "version": "3.1.0",
  "license": "MIT",
  "homepage": "https://github.com/personnummer/js",
  "author": {
    "name": "Fredrik Forsmo",
    "email": "fredrik.forsmo@gmail.com",
    "url": "https://frozzare.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/personnummer/js.git"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "NODE_ENV=production rollup -c",
    "build": "rimraf dist && npm run build:types && npm run build:js",
    "format": "prettier --write 'src/**/*.ts' test.ts",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/eslint-parser": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@jitesoft/babel-preset-main": "^2.5.1",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^13.0.2",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/jest": "^26.0.10",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^3.9.1",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.5",
    "eslint": "^7.7.0",
    "jest": "^26.4.0",
    "node-fetch": "^2.6.1",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.4",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  },
  "keywords": [
    "personnummer",
    "personal",
    "identity",
    "social",
    "security",
    "numbers"
  ]
}
