{
  "name": "personnummer",
  "description": "Validate Swedish personal identity numbers",
  "version": "3.0.2",
  "license": "MIT",
  "homepage": "https://github.com/personnummer/js",
  "author": {
    "name": "Fredrik Forsmo",
    "email": "fredrik.forsmo@gmail.com",
    "url": "https://frozzare.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/personnummer/js.git"
  },
  "scripts": {
    "babel:esm": "NODE_ENV=production BABEL_ENV=esm babel -d dist/esm src/ --extensions=.ts,.tsx --copy-files",
    "babel:cjs": "NODE_ENV=production babel -d dist/cjs src/ --extensions=.ts,.tsx --copy-files",
    "build": "rimraf dist && npm run babel:esm && npm run babel:cjs && tsc && rimraf dist/**/*.types.js dist/**/*.d.js",
    "format": "prettier --write 'src/**/*.ts' test.ts",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime-corejs3": "^7.9.6",
    "@jitesoft/babel-preset-main": "^2.2.2",
    "@types/jest": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.5",
    "eslint": "^7.0.0",
    "jest": "^25.5.4",
    "node-fetch": "^2.6.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "keywords": [
    "personnummer",
    "personal",
    "identity",
    "social",
    "security",
    "numbers"
  ],
  "semistandard": {
    "parser": "babel-eslint"
  }
}
